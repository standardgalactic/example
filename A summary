</span></span><span><span>            phi </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> np</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>random</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>randn</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>n</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span> </span><span class="token" style="color: rgb(97, 175, 239);">*</span><span> </span><span class="token" style="color: rgb(209, 154, 102);">2.0</span><span>
</span></span><span><span>            kappa </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> </span><span class="token" style="color: rgb(209, 154, 102);">0.2</span><span>
</span></span><span>        
</span><span><span>        </span><span class="token" style="color: rgb(152, 195, 121);">vars</span><span> </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> </span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span>
</span></span><span><span>        </span><span class="token" style="color: rgb(198, 120, 221);">for</span><span> t </span><span class="token" style="color: rgb(198, 120, 221);">in</span><span> </span><span class="token" style="color: rgb(152, 195, 121);">range</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span class="token" style="color: rgb(209, 154, 102);">1000</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span>
</span></span><span><span>            phi</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> kappa </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> step</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>phi</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> kappa</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> gamma</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span><span>            </span><span class="token" style="color: rgb(152, 195, 121);">vars</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>append</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>np</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>var</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>phi</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span>        
</span><span><span>        </span><span class="token" style="color: rgb(92, 99, 112); font-style: italic;"># Measure time to reach 95% of final value</span><span>
</span></span><span><span>        final_var </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> np</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>mean</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span class="token" style="color: rgb(152, 195, 121);">vars</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(97, 175, 239);">-</span><span class="token" style="color: rgb(209, 154, 102);">50</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span><span>        crossing </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> np</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>where</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>np</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span class="token" style="color: rgb(152, 195, 121);">abs</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>np</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>array</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span class="token" style="color: rgb(152, 195, 121);">vars</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span> </span><span class="token" style="color: rgb(97, 175, 239);">-</span><span> final_var</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span> </span><span class="token" style="color: rgb(97, 175, 239);">&lt;</span><span> </span><span class="token" style="color: rgb(209, 154, 102);">0.05</span><span> </span><span class="token" style="color: rgb(97, 175, 239);">*</span><span> final_var</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(209, 154, 102);">0</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span>
</span></span><span><span>        tau </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> crossing</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(209, 154, 102);">0</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span> </span><span class="token" style="color: rgb(198, 120, 221);">if</span><span> </span><span class="token" style="color: rgb(152, 195, 121);">len</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>crossing</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span> </span><span class="token" style="color: rgb(97, 175, 239);">&gt;</span><span> </span><span class="token" style="color: rgb(209, 154, 102);">0</span><span> </span><span class="token" style="color: rgb(198, 120, 221);">else</span><span> </span><span class="token" style="color: rgb(209, 154, 102);">1000</span><span>
</span></span><span><span>        tau_relax</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>append</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>tau </span><span class="token" style="color: rgb(97, 175, 239);">*</span><span> dt</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span>    
</span><span><span>    </span><span class="token" style="color: rgb(198, 120, 221);">return</span><span> np</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>array</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>tau_relax</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span>
</span><span><span>tau_up </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> measure_relaxation_time</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>gamma_vals</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(209, 154, 102);">10</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> </span><span class="token" style="color: rgb(152, 195, 121);">'coherent'</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span><span>tau_dn </span><span class="token" style="color: rgb(97, 175, 239);">=</span><span> measure_relaxation_time</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>gamma_vals</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(209, 154, 102);">10</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(97, 175, 239);">-</span><span class="token" style="color: rgb(209, 154, 102);">1</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> </span><span class="token" style="color: rgb(152, 195, 121);">'fragmented'</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span>
</span><span><span>plt</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>figure</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>figsize</span><span class="token" style="color: rgb(97, 175, 239);">=</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span class="token" style="color: rgb(209, 154, 102);">8</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> </span><span class="token" style="color: rgb(209, 154, 102);">5</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span><span>plt</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>plot</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>gamma_vals</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(209, 154, 102);">10</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> tau_up</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> </span><span class="token" style="color: rgb(152, 195, 121);">'o-'</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> color</span><span class="token" style="color: rgb(97, 175, 239);">=</span><span class="token" style="color: rgb(152, 195, 121);">'blue'</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> label</span><span class="token" style="color: rgb(97, 175, 239);">=</span><span class="token" style="color: rgb(152, 195, 121);">'From coherent'</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
</span></span><span><span>plt</span><span class="token" style="color: rgb(171, 178, 191);">.</span><span>plot</span><span class="token" style="color: rgb(171, 178, 191);">(</span><span>gamma_vals</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(209, 154, 102);">10</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span class="token" style="color: rgb(171, 178, 191);">[</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(171, 178, 191);">:</span><span class="token" style="color: rgb(97, 175, 239);">-</span><span class="token" style="color: rgb(209, 154, 102);">1</span><span class="token" style="color: rgb(171, 178, 191);">]</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> tau_dn</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> </span><span class="token" style="color: rgb(152, 195, 121);">'o-'</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> color</span><span class="token" style="color: rgb(97, 175, 239);">=</span><span class="token" style="color: rgb(152, 195, 121);">'red'</span><span class="token" style="color: rgb(171, 178, 191);">,</span><span> label</span><span class="token" style="color: rgb(97, 175, 239);">=</span><span class="token" style="color: rgb(152, 195, 121);">'From fragmented'</span><span class="token" style="color: rgb(171, 178, 191);">)</span><span>
