parser = argparse.ArgumentParser()
parser.add_argument(</span><span><span class="hljs-string">"--config"</span></span><span>, required=</span><span><span class="hljs-literal">True</span></span><span>)
parser.add_argument(</span><span><span class="hljs-string">"--out"</span></span><span>, required=</span><span><span class="hljs-literal">True</span></span><span>)
args, _ = parser.parse_known_args(sys.argv[sys.argv.index(</span><span><span class="hljs-string">"--"</span></span><span>) + </span><span><span class="hljs-number">1</span></span><span>:])

</span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(args.config) </span><span><span class="hljs-keyword">as</span></span><span> f:
    cfg = yaml.safe_load(f)

bpy.ops.wm.read_factory_settings(use_empty=</span><span><span class="hljs-literal">True</span></span><span>)

geom = cfg.get(</span><span><span class="hljs-string">"geometry"</span></span><span>, {}).get(</span><span><span class="hljs-string">"base"</span></span><span>, </span><span><span class="hljs-string">"sphere"</span></span><span>)
</span><span><span class="hljs-keyword">if</span></span><span> geom == </span><span><span class="hljs-string">"sphere"</span></span><span>:
    bpy.ops.mesh.primitive_uv_sphere_add(radius=</span><span><span class="hljs-number">1</span></span><span>)
</span><span><span class="hljs-keyword">elif</span></span><span> geom == </span><span><span class="hljs-string">"torus"</span></span><span>:
    bpy.ops.mesh.primitive_torus_add(major_radius=</span><span><span class="hljs-number">1</span></span><span>, minor_radius=</span><span><span class="hljs-number">0.3</span></span><span>)
obj = bpy.context.active_object
obj.name = cfg[</span><span><span class="hljs-string">"name"</span></span><span>]

</span><span><span class="hljs-comment"># Procedural deformation based on scalar field</span></span><span>
amp = cfg[</span><span><span class="hljs-string">"scalar_field"</span></span><span>][</span><span><span class="hljs-string">"amplitude"</span></span><span>]
sig = cfg[</span><span><span class="hljs-string">"scalar_field"</span></span><span>][</span><span><span class="hljs-string">"sigma"</span></span><span>]

</span><span><span class="hljs-keyword">for</span></span><span> v </span><span><span class="hljs-keyword">in</span></span><span> obj.data.vertices:
