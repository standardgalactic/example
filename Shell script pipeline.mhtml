    </span><span><span class="hljs-keyword">return</span></span><span> ap.parse_known_args(sys.argv[sys.argv.index(</span><span><span class="hljs-string">"--"</span></span><span>)+</span><span><span class="hljs-number">1</span></span><span>:])[</span><span><span class="hljs-number">0</span></span><span>]

</span><span><span class="hljs-comment"># -------- Field evaluators (procedural, CPU-side sampled per vertex) --------</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">eval_phi</span></span><span>(</span><span><span class="hljs-params">pt, spec</span></span><span>):
    t = spec.get(</span><span><span class="hljs-string">"type"</span></span><span>, </span><span><span class="hljs-string">"gaussian"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> t == </span><span><span class="hljs-string">"gaussian"</span></span><span>:
        A = spec.get(</span><span><span class="hljs-string">"amplitude"</span></span><span>, </span><span><span class="hljs-number">1.0</span></span><span>)
        sigma = spec.get(</span><span><span class="hljs-string">"sigma"</span></span><span>, </span><span><span class="hljs-number">0.6</span></span><span>)
        cx, cy, cz = spec.get(</span><span><span class="hljs-string">"center"</span></span><span>, [</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">0</span></span><span>])
        dx, dy, dz = pt[</span><span><span class="hljs-number">0</span></span><span>]-cx, pt[</span><span><span class="hljs-number">1</span></span><span>]-cy, pt[</span><span><span class="hljs-number">2</span></span><span>]-cz
        r2 = dx*dx + dy*dy + dz*dz
        </span><span><span class="hljs-keyword">return</span></span><span> A * math.exp(-r2/(</span><span><span class="hljs-number">2</span></span><span>*sigma*sigma))
    </span><span><span class="hljs-keyword">if</span></span><span> t == </span><span><span class="hljs-string">"radial"</span></span><span>:
        scale = spec.get(</span><span><span class="hljs-string">"scale"</span></span><span>, </span><span><span class="hljs-number">1.0</span></span><span>)
        r = sqrt(pt[</span><span><span class="hljs-number">0</span></span><span>]**</span><span><span class="hljs-number">2</span></span><span>+pt[</span><span><span class="hljs-number">1</span></span><span>]**</span><span><span class="hljs-number">2</span></span><span>+pt[</span><span><span class="hljs-number">2</span></span><span>]**</span><span><span class="hljs-number">2</span></span><span>)
        </span><span><span class="hljs-keyword">return</span></span><span> math.exp(-scale*r)
    </span><span><span class="hljs-keyword">if</span></span><span> t == </span><span><span class="hljs-string">"perlin"</span></span><span>:
        </span><span><span class="hljs-comment"># Minimal placeholder; replace with real noise if desired</span></span><span>
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">0.5</span></span><span>
