</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">hexcolor</span></span><span>(</span><span><span class="hljs-params">rgb</span></span><span>):
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">'#%02x%02x%02x'</span></span><span> % </span><span><span class="hljs-built_in">tuple</span></span><span>(</span><span><span class="hljs-built_in">int</span></span><span>(x) </span><span><span class="hljs-keyword">for</span></span><span> x </span><span><span class="hljs-keyword">in</span></span><span> rgb)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">estimate_entropy_band</span></span><span>(</span><span><span class="hljs-params">phi_mean</span></span><span>):
    </span><span><span class="hljs-string">"""Classify average entropy band by normalized luminance."""</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> phi_mean &gt; </span><span><span class="hljs-number">0.5</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">"A"</span></span><span>  </span><span><span class="hljs-comment"># Eloi open diffusion</span></span><span>
    </span><span><span class="hljs-keyword">elif</span></span><span> </span><span><span class="hljs-number">0.0</span></span><span> &lt; phi_mean &lt;= </span><span><span class="hljs-number">0.5</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">"B"</span></span><span>  </span><span><span class="hljs-comment"># equilibrium</span></span><span>
    </span><span><span class="hljs-keyword">elif</span></span><span> -</span><span><span class="hljs-number">0.5</span></span><span> &lt;= phi_mean &lt;= </span><span><span class="hljs-number">0.0</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">"D"</span></span><span>  </span><span><span class="hljs-comment"># regenerative transition</span></span><span>
    </span><span><span class="hljs-keyword">else</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">"C"</span></span><span>  </span><span><span class="hljs-comment"># negentropic inversion</span></span><span>

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">analyze_images</span></span><span>(</span><span><span class="hljs-params">scene_dir</span></span><span>):
    </span><span><span class="hljs-string">"""Compute phi_range and dominant colors from .png frames."""</span></span><span>
    imgs = </span><span><span class="hljs-built_in">sorted</span></span><span>(glob.glob(os.path.join(scene_dir, </span><span><span class="hljs-string">"frame_*.png"</span></span><span>)))
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> imgs:
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-literal">None</span></span><span>
    lums = []
    color_samples = []
    </span><span><span class="hljs-keyword">for</span></span><span> path </span><span><span class="hljs-keyword">in</span></span><span> imgs[:</span><span><span class="hljs-built_in">min</span></span><span>(</span><span><span class="hljs-number">10</span></span><span>, </span><span><span class="hljs-built_in">len</span></span><span>(imgs))]:  </span><span><span class="hljs-comment"># sample first 10 frames</span></span><span>
        </span><span><span class="hljs-keyword">with</span></span><span> Image.</span><span><span class="hljs-built_in">open</span></span><span>(path) </span><span><span class="hljs-keyword">as</span></span><span> im:
            im = im.convert(</span><span><span class="hljs-string">"RGB"</span></span><span>).resize((</span><span><span class="hljs-number">64</span></span><span>,</span><span><span class="hljs-number">64</span></span><span>))
            px = </span><span><span class="hljs-built_in">list</span></span><span>(im.getdata())
            ls = [luminance(p)/</span><span><span class="hljs-number">255.0</span></span><span>*</span><span><span class="hljs-number">2</span></span><span>-</span><span><span class="hljs-number">1</span></span><span> </span><span><span class="hljs-keyword">for</span></span><span> p </span><span><span class="hljs-keyword">in</span></span><span> px]  </span><span><span class="hljs-comment"># normalize to [-1,1]</span></span><span>
            lums.extend(ls)
            color_samples.extend(px)
    phi_min, phi_max = </span><span><span class="hljs-built_in">round</span></span><span>(</span><span><span class="hljs-built_in">min</span></span><span>(lums),</span><span><span class="hljs-number">3</span></span><span>), </span><span><span class="hljs-built_in">round</span></span><span>(</span><span><span class="hljs-built_in">max</span></span><span>(lums),</span><span><span class="hljs-number">3</span></span><span>)
    phi_mean = (phi_min + phi_max)/</span><span><span class="hljs-number">2</span></span><span>
    band = estimate_entropy_band(phi_mean)
    </span><span><span class="hljs-comment"># dominant colors</span></span><span>
    common = Counter([</span><span><span class="hljs-built_in">tuple</span></span><span>(</span><span><span class="hljs-built_in">map</span></span><span>(</span><span><span class="hljs-keyword">lambda</span></span><span> v: </span><span><span class="hljs-built_in">int</span></span><span>(v/</span><span><span class="hljs-number">32</span></span><span>)*</span><span><span class="hljs-number">32</span></span><span>, c)) </span><span><span class="hljs-keyword">for</span></span><span> c </span><span><span class="hljs-keyword">in</span></span><span> color_samples])
    palette = [hexcolor(c) </span><span><span class="hljs-keyword">for</span></span><span> c,_ </span><span><span class="hljs-keyword">in</span></span><span> common.most_common(</span><span><span class="hljs-number">3</span></span><span>)]
    </span><span><span class="hljs-keyword">return</span></span><span> {
        </span><span><span class="hljs-string">"phi_range"</span></span><span>: [phi_min, phi_max],
        </span><span><span class="hljs-string">"entropy_band"</span></span><span>: band,
        </span><span><span class="hljs-string">"color_palette"</span></span><span>: palette
    }

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">update_manifest</span></span><span>(</span><span><span class="hljs-params">scene_dir, derived</span></span><span>):
    path = os.path.join(scene_dir, </span><span><span class="hljs-string">"MANIFEST.jsonl"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> os.path.exists(path):
        </span><span><span class="hljs-keyword">return</span></span><span>
    lines = []
    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(path) </span><span><span class="hljs-keyword">as</span></span><span> f:
        </span><span><span class="hljs-keyword">for</span></span><span> line </span><span><span class="hljs-keyword">in</span></span><span> f:
            </span><span><span class="hljs-keyword">try</span></span><span>:
                entry = json.loads(line)
                entry.update(derived)
                lines.append(entry)
            </span><span><span class="hljs-keyword">except</span></span><span> Exception:
                lines.append(line)
    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(path, </span><span><span class="hljs-string">"w"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
        </span><span><span class="hljs-keyword">for</span></span><span> entry </span><span><span class="hljs-keyword">in</span></span><span> lines:
            f.write(json.dumps(entry) + </span><span><span class="hljs-string">"\n"</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">main</span></span><span>():
    parser = argparse.ArgumentParser()
    parser.add_argument(</span><span><span class="hljs-string">"--root"</span></span><span>, default=</span><span><span class="hljs-string">"output"</span></span><span>, </span><span><span class="hljs-built_in">help</span></span><span>=</span><span><span class="hljs-string">"Root directory of appliances"</span></span><span>)
    args = parser.parse_args()
    dirs = </span><span><span class="hljs-built_in">sorted</span></span><span>(glob.glob(os.path.join(args.root, </span><span><span class="hljs-string">"appliances_*"</span></span><span>, </span><span><span class="hljs-string">"appliance_*"</span></span><span>)))
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> dirs:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"[WARN] No appliance directories found."</span></span><span>)
        </span><span><span class="hljs-keyword">return</span></span><span>
    </span><span><span class="hljs-keyword">for</span></span><span> d </span><span><span class="hljs-keyword">in</span></span><span> dirs:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"[INFO] Analyzing <span class="hljs-subst">{os.path.basename(d)}</span></span></span><span> ...")
        derived = analyze_images(d)
        </span><span><span class="hljs-keyword">if</span></span><span> derived:
            </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"  _range=<span class="hljs-subst">{derived[<span class="hljs-string">'phi_range'</span></span></span></span><span>]}, band=</span><span><span class="hljs-subst">{derived[<span class="hljs-string">'entropy_band'</span></span></span><span>]}, palette=</span><span><span class="hljs-subst">{derived[<span class="hljs-string">'color_palette'</span></span></span><span>]}")
            update_manifest(d, derived)
    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"[OK] MANIFESTs updated with empirical entropy metadata."</span></span><span>)

</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    main()
</span></span></code></div></div></pre>
<hr data-start="3948" data-end="3951">
<h2 data-start="3953" data-end="3971"> What It Does</h2>
<ol data-start="3973" data-end="4333">
<li data-start="3973" data-end="4046">
<p data-start="3976" data-end="4046"><strong data-start="3976" data-end="3985">Scans</strong> every <code data-start="3992" data-end="4005">appliance_*</code> directory under <code data-start="4022" data-end="4043">output/appliances_*</code>.</p>
</li>
<li data-start="4047" data-end="4110">
<p data-start="4050" data-end="4110"><strong data-start="4050" data-end="4088">Reads up to 10 rendered PNG frames</strong> (to minimize load).</p>
</li>
<li data-start="4111" data-end="4174">
<p data-start="4114" data-end="4174">Converts each to luminance  entropy proxy (  [1, 1]).</p>
</li>
<li data-start="4175" data-end="4276">
<p data-start="4178" data-end="4276"><strong data-start="4178" data-end="4202">Computes <code data-start="4189" data-end="4200">phi_range</code></strong>, <strong data-start="4204" data-end="4233">classifies <code data-start="4217" data-end="4231">entropy_band</code></strong>, and <strong data-start="4239" data-end="4273">extracts 3 dominant hex colors</strong>.</p>
</li>
<li data-start="4277" data-end="4333">
<p data-start="4280" data-end="4333">Appends this data into each <code data-start="4308" data-end="4324">MANIFEST.jsonl</code> entry.</p>
</li>
</ol>
