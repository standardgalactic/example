</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">hash_file</span></span><span>(</span><span><span class="hljs-params">path</span></span><span>):
    h = hashlib.sha256()
    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(path, </span><span><span class="hljs-string">"rb"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
        </span><span><span class="hljs-keyword">for</span></span><span> chunk </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">iter</span></span><span>(</span><span><span class="hljs-keyword">lambda</span></span><span>: f.read(</span><span><span class="hljs-number">8192</span></span><span>), </span><span><span class="hljs-string">b""</span></span><span>):
            h.update(chunk)
    </span><span><span class="hljs-keyword">return</span></span><span> h.hexdigest()[:</span><span><span class="hljs-number">16</span></span><span>]

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">load_manifest</span></span><span>(</span><span><span class="hljs-params">scene_dir</span></span><span>):
    path = os.path.join(scene_dir, </span><span><span class="hljs-string">"MANIFEST.jsonl"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> os.path.exists(path):
        </span><span><span class="hljs-keyword">return</span></span><span> []
    </span><span><span class="hljs-keyword">try</span></span><span>:
        </span><span><span class="hljs-keyword">return</span></span><span> [json.loads(line) </span><span><span class="hljs-keyword">for</span></span><span> line </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(path)]
    </span><span><span class="hljs-keyword">except</span></span><span> Exception:
        </span><span><span class="hljs-keyword">return</span></span><span> []

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">interpret_metadata</span></span><span>(</span><span><span class="hljs-params">meta</span></span><span>):
    </span><span><span class="hljs-string">"""Generate a symbolic description based on entropy_band, phi_range, and color_palette."""</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> meta:
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">""</span></span><span>
    desc = []
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-string">"entropy_band"</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> meta:
        band = meta[</span><span><span class="hljs-string">"entropy_band"</span></span><span>]
        meaning = {
            </span><span><span class="hljs-string">"A"</span></span><span>: </span><span><span class="hljs-string">"open diffusion (Eloi)"</span></span><span>,
            </span><span><span class="hljs-string">"B"</span></span><span>: </span><span><span class="hljs-string">"equilibrium transition"</span></span><span>,
            </span><span><span class="hljs-string">"C"</span></span><span>: </span><span><span class="hljs-string">"negentropic inversion (Morlock)"</span></span><span>,
            </span><span><span class="hljs-string">"D"</span></span><span>: </span><span><span class="hljs-string">"regenerative pulse"</span></span><span>,
        }.get(band, </span><span><span class="hljs-string">"undetermined state"</span></span><span>)
        desc.append(</span><span><span class="hljs-string">f"Entropy band **<span class="hljs-subst">{band}</span></span></span><span>**  </span><span><span class="hljs-subst">{meaning}</span></span><span>.")
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-string">"phi_range"</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> meta:
        </span><span><span class="hljs-keyword">try</span></span><span>:
            low, high = meta[</span><span><span class="hljs-string">"phi_range"</span></span><span>]
            mid = (low + high) / </span><span><span class="hljs-number">2</span></span><span>
            </span><span><span class="hljs-keyword">if</span></span><span> mid &gt; </span><span><span class="hljs-number">0.5</span></span><span>:
                desc.append(</span><span><span class="hljs-string">"Dominant **exergic flow** (positive )."</span></span><span>)
            </span><span><span class="hljs-keyword">elif</span></span><span> mid &lt; -</span><span><span class="hljs-number">0.5</span></span><span>:
                desc.append(</span><span><span class="hljs-string">"Dominant **negentropic flux** (negative )."</span></span><span>)
            </span><span><span class="hljs-keyword">else</span></span><span>:
                desc.append(</span><span><span class="hljs-string">"Balanced entropic potential."</span></span><span>)
        </span><span><span class="hljs-keyword">except</span></span><span> Exception:
            </span><span><span class="hljs-keyword">pass</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-string">"color_palette"</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> meta:
        pal = meta[</span><span><span class="hljs-string">"color_palette"</span></span><span>]
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">isinstance</span></span><span>(pal, </span><span><span class="hljs-built_in">list</span></span><span>) </span><span><span class="hljs-keyword">and</span></span><span> </span><span><span class="hljs-built_in">len</span></span><span>(pal) &gt;= </span><span><span class="hljs-number">2</span></span><span>:
            desc.append(</span><span><span class="hljs-string">f"Color span <span class="hljs-subst">{pal[<span class="hljs-number">0</span></span></span></span><span>]}  </span><span><span class="hljs-subst">{pal[-<span class="hljs-number">1</span></span></span><span>]}.")
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">" "</span></span><span>.join(desc) </span><span><span class="hljs-keyword">if</span></span><span> desc </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-string">""</span></span><span>

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">find_appliance_dirs</span></span><span>(</span><span><span class="hljs-params">root=<span class="hljs-string">"output"</span></span></span><span>):
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">sorted</span></span><span>(glob.glob(os.path.join(root, </span><span><span class="hljs-string">"appliances_*"</span></span><span>, </span><span><span class="hljs-string">"appliance_*"</span></span><span>)))

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">make_catalog</span></span><span>():
    dirs = find_appliance_dirs()
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> dirs:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"[WARN] No appliance outputs found."</span></span><span>)
        </span><span><span class="hljs-keyword">return</span></span><span>
    now = datetime.datetime.utcnow().strftime(</span><span><span class="hljs-string">"%Y-%m-%d %H:%M:%SZ"</span></span><span>)
    lines = [
        </span><span><span class="hljs-string">"#  Futuristic Appliance Catalog"</span></span><span>,
        </span><span><span class="hljs-string">f"_Generated automatically on <span class="hljs-subst">{now}</span></span></span><span>_",
        </span><span><span class="hljs-string">""</span></span><span>,
        </span><span><span class="hljs-string">"| Appliance | SHA-256 | Date | Symbolic Description | Preview |"</span></span><span>,
        </span><span><span class="hljs-string">"|:-----------|:--------|:------|:--------------------|:---------|"</span></span><span>,
    ]
    </span><span><span class="hljs-keyword">for</span></span><span> d </span><span><span class="hljs-keyword">in</span></span><span> dirs:
        name = os.path.basename(d)
        blend = </span><span><span class="hljs-built_in">next</span></span><span>((f </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> glob.glob(os.path.join(d, </span><span><span class="hljs-string">"*.blend"</span></span><span>))), </span><span><span class="hljs-literal">None</span></span><span>)
        mp4 = </span><span><span class="hljs-built_in">next</span></span><span>((f </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> glob.glob(os.path.join(d, </span><span><span class="hljs-string">"*.mp4"</span></span><span>))), </span><span><span class="hljs-literal">None</span></span><span>)
        sha = hash_file(blend) </span><span><span class="hljs-keyword">if</span></span><span> blend </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-string">""</span></span><span>
        date = datetime.datetime.utcfromtimestamp(os.path.getmtime(d)).strftime(</span><span><span class="hljs-string">"%Y-%m-%d"</span></span><span>)

        </span><span><span class="hljs-comment"># Load first manifest record if exists</span></span><span>
        manifest = load_manifest(d)
        meta = manifest[</span><span><span class="hljs-number">0</span></span><span>] </span><span><span class="hljs-keyword">if</span></span><span> manifest </span><span><span class="hljs-keyword">else</span></span><span> {}

        </span><span><span class="hljs-comment"># Choose description</span></span><span>
        desc = SYMBOLIC_OVERRIDES.get(name)
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> desc:
            desc = interpret_metadata(meta)

        preview = </span><span><span class="hljs-string">f"![](<span class="hljs-subst">{mp4}</span></span></span><span>)" </span><span><span class="hljs-keyword">if</span></span><span> mp4 </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-string">""</span></span><span>
        lines.append(</span><span><span class="hljs-string">f"| `<span class="hljs-subst">{name}</span></span></span><span>` | `</span><span><span class="hljs-subst">{sha}</span></span><span>` | </span><span><span class="hljs-subst">{date}</span></span><span> | </span><span><span class="hljs-subst">{desc}</span></span><span> | </span><span><span class="hljs-subst">{preview}</span></span><span> |")

    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(</span><span><span class="hljs-string">"CATALOG.md"</span></span><span>, </span><span><span class="hljs-string">"w"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
        f.write(</span><span><span class="hljs-string">"\n"</span></span><span>.join(lines))
    </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"[OK] CATALOG.md written with <span class="hljs-subst">{<span class="hljs-built_in">len</span></span></span></span><span>(dirs)} entries and semantic metadata.")

</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    make_catalog()
</span></span></code></div></div></pre>
