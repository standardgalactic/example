</span><span><span class="hljs-keyword">import</span></span><span> bpy, argparse, sys, math, json, os
</span><span><span class="hljs-keyword">from</span></span><span> math </span><span><span class="hljs-keyword">import</span></span><span> sin, cos, pi, exp, sqrt
</span><span><span class="hljs-keyword">import</span></span><span> yaml

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">parse_args</span></span><span>():
    ap = argparse.ArgumentParser()
