    phi = clamp(phi, -1.0, 1.0);
    if(phi&gt;0.7)  return mix(vec3(0.45,0.90,0.65), vec3(0.85,1.00,0.85), (phi-0.7)/0.3);
    if(phi&gt;0.5)  return mix(vec3(0.25,0.35,0.45), vec3(0.55,0.95,1.00), (phi-0.5)/0.2);
    if(phi&gt;0.0)  return mix(vec3(0.35,0.35,0.40), vec3(0.25,0.35,0.45), phi/0.5);
    if(phi&gt;-0.5) return mix(vec3(0.18,0.14,0.35), vec3(0.35,0.35,0.40), (phi+0.5)/0.5);
    if(phi&gt;-0.8) return mix(vec3(0.10,0.05,0.20), vec3(0.18,0.14,0.35), (phi+0.8)/0.3);
    if(phi&gt;-1.0) return mix(vec3(0.10,1.00,0.90), vec3(0.10,0.05,0.20), (phi+1.0)/0.2);
    return vec3(0.0);
}
