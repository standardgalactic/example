      </span><span><span class="hljs-attr">id-token:</span></span><span> </span><span><span class="hljs-string">write</span></span><span>
    </span><span><span class="hljs-attr">strategy:</span></span><span>
      </span><span><span class="hljs-attr">fail-fast:</span></span><span> </span><span><span class="hljs-literal">false</span></span><span>
      </span><span><span class="hljs-attr">matrix:</span></span><span>
        </span><span><span class="hljs-attr">scene:</span></span><span>
          </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-string">data/lamphrodyne.yaml</span></span><span>
          </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-string">data/entropic_vortex.yaml</span></span><span>
