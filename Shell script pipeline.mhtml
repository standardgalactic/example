</span><span><span class="hljs-attr">on:</span></span><span>
  </span><span><span class="hljs-attr">workflow_dispatch:</span></span><span>
  </span><span><span class="hljs-attr">push:</span></span><span>
    </span><span><span class="hljs-attr">paths:</span></span><span>
      </span><span><span class="hljs-bullet">-</span></span><span> </span><span><span class="hljs-string">'scripts/**'</span></span><span>
