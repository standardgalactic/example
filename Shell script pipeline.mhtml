</span><span><span class="hljs-keyword">import</span></span><span> argparse, numpy </span><span><span class="hljs-keyword">as</span></span><span> np

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">write_ply_xyz</span></span><span>(</span><span><span class="hljs-params">path, pts</span></span><span>):
    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(path, </span><span><span class="hljs-string">"w"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
