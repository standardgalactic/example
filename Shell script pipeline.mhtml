    </span><span><span class="hljs-keyword">return</span></span><span> ap.parse_known_args(sys.argv[sys.argv.index(</span><span><span class="hljs-string">"--"</span></span><span>)+</span><span><span class="hljs-number">1</span></span><span>:])[</span><span><span class="hljs-number">0</span></span><span>]

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">breathe_over_time</span></span><span>(</span><span><span class="hljs-params">frame, fps, amp, freq_hz</span></span><span>):
    t = frame/</span><span><span class="hljs-built_in">float</span></span><span>(fps)
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-number">1.0</span></span><span> + amp * math.sin(</span><span><span class="hljs-number">2</span></span><span>*math.pi*freq_hz*t)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">update_phi_on_mesh</span></span><span>(</span><span><span class="hljs-params">obj, name_phi, base_values, scale</span></span><span>):
    me = obj.data
    a_phi = me.attributes[name_phi].data
    </span><span><span class="hljs-keyword">for</span></span><span> i, val </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">enumerate</span></span><span>(base_values):
        a_phi[i].value = val * scale
    me.update()

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">cache_base_phi</span></span><span>(</span><span><span class="hljs-params">obj, name_phi</span></span><span>):
    me = obj.data
    </span><span><span class="hljs-keyword">return</span></span><span> [me.attributes[name_phi].data[i].value </span><span><span class="hljs-keyword">for</span></span><span> i,_ </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">enumerate</span></span><span>(me.vertices)]

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">main</span></span><span>():
    args = parse_args()
    </span><span><span class="hljs-keyword">with</span></span><span> </span><span><span class="hljs-built_in">open</span></span><span>(args.config,</span><span><span class="hljs-string">"r"</span></span><span>) </span><span><span class="hljs-keyword">as</span></span><span> f:
        cfg = yaml.safe_load(f)

    scene = bpy.context.scene
    frames = </span><span><span class="hljs-built_in">int</span></span><span>(cfg.get(</span><span><span class="hljs-string">"animation"</span></span><span>,{}).get(</span><span><span class="hljs-string">"frames"</span></span><span>, </span><span><span class="hljs-number">160</span></span><span>))
    scene.frame_end = frames
    scene.render.image_settings.file_format = </span><span><span class="hljs-string">'FFMPEG'</span></span><span>
    scene.render.filepath = </span><span><span class="hljs-string">f"<span class="hljs-subst">{args.out}</span></span></span><span>/animation.mp4"

    </span><span><span class="hljs-comment"># Camera</span></span><span>
    cam_radius = </span><span><span class="hljs-built_in">float</span></span><span>(cfg.get(</span><span><span class="hljs-string">"animation"</span></span><span>,{}).get(</span><span><span class="hljs-string">"camera_radius"</span></span><span>, </span><span><span class="hljs-number">3.0</span></span><span>))
    bpy.ops.</span><span><span class="hljs-built_in">object</span></span><span>.camera_add(location=(cam_radius, </span><span><span class="hljs-number">0</span></span><span>, cam_radius*</span><span><span class="hljs-number">0.7</span></span><span>))
    cam = bpy.context.active_object
