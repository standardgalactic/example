    v.co *= (</span><span><span class="hljs-number">1</span></span><span> + </span><span><span class="hljs-number">0.2</span></span><span> * d)

</span><span><span class="hljs-comment"># Color or emission material from entropy field</span></span><span>
mat = bpy.data.materials.new(</span><span><span class="hljs-string">"EntropyEmission"</span></span><span>)
mat.use_nodes = </span><span><span class="hljs-literal">True</span></span><span>
nodes = mat.node_tree.nodes
emission = nodes.new(</span><span><span class="hljs-string">"ShaderNodeEmission"</span></span><span>)
emission.inputs[</span><span><span class="hljs-string">"Strength"</span></span><span>].default_value = cfg[</span><span><span class="hljs-string">"visual"</span></span><span>][</span><span><span class="hljs-string">"emission_strength"</span></span><span>]
mat.node_tree.links.new(emission.outputs[</span><span><span class="hljs-string">"Emission"</span></span><span>], nodes[</span><span><span class="hljs-string">"Material Output"</span></span><span>].inputs[</span><span><span class="hljs-string">"Surface"</span></span><span>])
obj.data.materials.append(mat)

bpy.ops.wm.save_as_mainfile(filepath=args.out)
</span></span></code></div></div></pre>
