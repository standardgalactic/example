    </span><span><span class="hljs-keyword">return</span></span><span> (</span><span><span class="hljs-number">0.0</span></span><span>,</span><span><span class="hljs-number">0.0</span></span><span>,</span><span><span class="hljs-number">0.0</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">eval_S</span></span><span>(</span><span><span class="hljs-params">pt, spec</span></span><span>):
