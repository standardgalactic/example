    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">N2</span></span><span>(</span><span><span class="hljs-params">pt</span></span><span>): </span><span><span class="hljs-keyword">return</span></span><span> perlin.noise(pt[</span><span><span class="hljs-number">0</span></span><span>]+</span><span><span class="hljs-number">31.7</span></span><span>,pt[</span><span><span class="hljs-number">1</span></span><span>]-</span><span><span class="hljs-number">12.3</span></span><span>,pt[</span><span><span class="hljs-number">2</span></span><span>]+</span><span><span class="hljs-number">7.1</span></span><span>, freq=freq)  </span><span><span class="hljs-comment"># shifts = independent channels</span></span><span>
